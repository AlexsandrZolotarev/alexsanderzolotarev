{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"], 
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
 "include": ["src", "src/types"]
}
